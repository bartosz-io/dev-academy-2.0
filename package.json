{
  "name": "dev-academy-2.0",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install:web-security": "npm --prefix ./nested-source/web-security install",
    "install": "npm run install:web-security",
    "dev:web-security": "npm --prefix ./nested-source/web-security run dev",
    "build:web-security": "npm --prefix ./nested-source/web-security run build",
    "build:hexo": "hexo generate --config _config.yml,_prod.yml",
    "build": "npm run build:web-security && npm run build:hexo",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server --config _config.yml,_dev.yml",
    "article": "hexo new post"
  },
  "hexo": {
    "version": "5.4.2"
  },
  "dependencies": {
    "hexo": "^5.0.0",
    "hexo-all-minifier": "^0.5.7",
    "hexo-auto-canonical": "^0.1.1",
    "hexo-browsersync": "^0.3.0",
    "hexo-feed": "^1.1.0",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-filter-responsive-images": "^1.7.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-seo-friendly-sitemap": "^0.2.1",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^3.3.0",
    "hexo-renderer-scss": "^1.2.0",
    "hexo-server": "^2.0.0",
    "hexo-toc": "^1.1.0",
    "hexo-util": "^2.5.0",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "minimatch": "^3.0.4",
    "posthog-js": "^1.43.1",
    "sharp": "^0.31.1"
  }
